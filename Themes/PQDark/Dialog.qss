QDialog {
    background: #2b2b2b;
    border: 1px solid #c4c4c4;
    font-family: FontAwesome;
}

QDialog QLabel {
     color: #c4c4c4;
     font-size: 14px;
}

QDialog QPushButton {
    background: #2b2b2b;
    color: #c4c4c4;
    font-size: 14px;
    border: 1px solid #393939;
    border-radius: 3px;
    height: 20px;
    padding-left: 10px;
    padding-right: 10px;
}

QDialog QPushButton:hover {
    background: #8a3b16;
}